    "reek": true,
    "rubocop": true,
    "ruby": true, //Runs ruby -wc
    "fasterer": true,
    "debride": true,
    "ruby-lint": true
  },
  "workbench.colorTheme": "Solarized Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": true,
  "workbench.panel.defaultLocation": "right",
  "editor.suggestSelection": "first",
